# .env.example
# This file serves as a template for the required environment variables.
# Copy this file to .env and fill in your actual values. DO NOT commit .env file.

# --- MySQL Database Configuration ---
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=your_mysql_password
DB_DATABASE=compressor_db

# --- InfluxDB Configuration ---
INFLUXDB_URL=http://localhost:8086
INFLUXDB_TOKEN=your_influxdb_token
INFLUXDB_ORG=your_influxdb_org
INFLUXDB_BUCKET=your_influxdb_bucket

# --- Kafka Configuration ---
KAFKA_BOOTSTRAP_SERVERS=localhost:9092
KAFKA_DVR_GROUP_ID=dvr-group

# Model Paths
STATUS_MODEL_PATH="backend/compressor_status_prediction_model.onnx"
DART_MODEL_PATH="backend/dart_model.onnx"
